{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/src/**",
                "/usr/arm-none-eabi/include/**"
            ],
            "defines": [
                "SYSCLK_FREQ_72MHz",
                "STM32F103C8",
                "STM32F10X_MD",
                "USE_STDPERIPH_DRIVER",
                "__ASSEMBLY__"
            ],
            "compilerPath": "/usr/bin/arm-none-eabi-gcc",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "gcc-x64"
        }
    ],
    "version": 4
}

